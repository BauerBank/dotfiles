profile {
	output $INTERNAL enable
  output $ANKER1 enable
  output $ANKER2 enable
}

output "Lenovo Group Limited 0x403A Unknown" {
    mode 1920x1200@60.00
    position 0,0
    scale 1
    alias $INTERNAL
}

output "AOC G2460PG #ASPrfPN4lODd" {
    mode 1920x1080@143.98
    position -1920,0
    scale 1
    alias $ANKER1
}

output "Acer Technologies V223PW LN80C0044013" {
    mode 1680x1050@59.95
    position -3600,0
    scale 1
    alias $ANKER2
}
